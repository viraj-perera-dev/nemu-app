{
  "expo": {
    "name": "nemù",
    "slug": "nemu-app",
    "description": "Nemù app description",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#272727"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "nemu.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#272727"
      },
      "package": "nemu.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9da35b1e-3dc3-4226-9780-a27667c5496b"
      }
    },
    "owner": "virajdev",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9da35b1e-3dc3-4226-9780-a27667c5496b"
    }
  }
}
